<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}band-profile{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('css/application.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap-4.css') }}">
    {% endblock %}
    <script async src="https://www.googletagmanager.com/gtag/js?id={{ ga_tracking }}"></script>
    <script>
        window.dataLayer = window.dataLayer || [];

        function gtag() {
            dataLayer.push(arguments);
        }

        gtag('js', new Date());

        gtag('config', '{{ ga_tracking }}');
    </script>
</head>
<body>
{% block body %}
{% endblock %}
</body>
{% block javascripts %}
    <script src="{{ asset('js/application.js') }}"></script>
    <script src="{{ asset('js/bootstrap-4.js') }}"></script>
{% endblock %}
</html>
